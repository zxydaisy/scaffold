# "extends": "airbnb-base"
{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "globals": {
    "React": true,
    "ReactDOM": true,
    "$": true,
    "_": true,
    "IcoPath",
  },
  "rules": {
    "indent": ["error", 2],
    "no-console": ["error", { allow: ["log", "warn", "error"] }],
    "arrow-body-style": 0,
    "quote-props": ["error", "as-needed"],
    "camelcase": ["error", { properties: "never" }],
    "react/no-multi-comp": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "spaced-comment": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-class-assign": 0
  },
}
